{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Consul": {
    "Host": "localhost",
    "Port": 8500,
    "Configuration": {
      "EnabledRefresh": true,
      "RefreshInterval": 5,
      "Format": "JSON",
      "DataKey": "Configuration"
    },
    "Discovery": {
      "Enabled": true,
      "Register": false,
      "Port": 44367,
      "CatalogServicesWatchDelay": 10,
      "HealthCheckInterval": "30s",
      "HealthCheckPath": "api/Consul/health",
      "FailFast": true,
      "ServiceName": "NavyBlue.WebTest",
      "InstanceId": "NavyBlue.WebTest-{0}:44367", //ServiceName-IP:Port
      "PreferIpAddress": true
    }
  }
}
